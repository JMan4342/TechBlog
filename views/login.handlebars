<div class="card col-8 border-dark mb-3 mx-auto">
  <h2 class="row justify-content-between p-3" id="cardHead">Login</h2>

  <form class="form login-form mx-3">
    <div class="form-floating mb-3">
      <input
        class="form-control"
        type="text"
        name="username-login"
        id="username-login"
      />
      <label for="username-login">Username</label>
    </div>
    <div class="form-floating">
      <input
        class="form-control"
        type="password"
        name="password-login"
        id="password-login"
      />
      <label for="password-login">Password</label>
    </div>
    <div class="form-group">
      <button
        class="btn btn-secondary btn-sm my-2"
        type="submit"
      >Login</button>
    </div>
    <div>
      <a
        class="btn btn-secondary btn-sm my-2"
        id="createBtn"
        href="../createAcct"
      >Create an Account</a>
    </div>
  </form>
</div>

<script src="/js/login.js"></script>